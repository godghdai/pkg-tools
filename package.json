    "which": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^8.5.7",
    "@types/request-promise": "^4.1.39",
    "@types/which": "^1.3.1",
    "concurrently": "^3.5.1",
    "nodemon": "^1.14.7",
    "typescript": "^2.6.2"
